generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Problemset {
  id                                String        @id @default(uuid()) 
  setName                           String
  problems                          problem []         

}

model problem {
  id                String          @id @default(ulid())
  problemSet        Problemset      @relation(fields: [problemSetId], references: [id])
  problemSetId      String
  problemName       String
  optionA           String 
  optionB           String 
  optionC           String 
  optionD           String 

}
